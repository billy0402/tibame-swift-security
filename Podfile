# Uncomment the next line to define a global platform for your project
 platform :ios, '11.4'

target 'MySecureApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MySecureApp
  pod 'RNCryptor', '~> 5.0'
  pod 'TrustKit', '~> 1.5'
  pod 'KeychainAccess', '~> 3.1'

  target 'MySecureAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MySecureAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
